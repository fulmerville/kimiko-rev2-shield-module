        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&mt TILDE ESC  &kp N1  &kp N2  &kp N3  &kp N4    &kp N5                                                        &kp N6  &kp N7  &kp N8     &kp N9   &kp N0         &mt MINUS EQUAL
&kp TAB        &kp Q   &kp W   &kp E   &kp R     &kp T                                                         &kp Y   &kp U   &kp I      &kp O    &kp P          &mt RBKT LBKT
&kp LSHIFT     &kp A   &kp S   &kp D   &kp F     &kp G                                                         &kp H   &kp J   &kp K      &kp L    &kp SEMI       &mt RSHFT APOS
&kp LCTRL      &kp Z   &kp X   &kp C   &kp V     &kp B     &none      &none    &none          &kp PRINTSCREEN  &kp N   &kp M   &kp COMMA  &kp DOT  &mt BSLH FSLH  &kp RCTRL
                               &none   &kp LGUI  &kp LALT  &kp SPACE  &kp DEL  &kp BACKSPACE  &kp RET          &tog 1  &tog 2  &none
            >;
